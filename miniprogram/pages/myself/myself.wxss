/* miniprogram/pages/myself/myself.wxss */
page {
  background:#f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.userinfo, .uploader, .tunnel {
  margin-top: 40rpx;
  height: 1140rpx;
  width: 100%;
  background: #fff;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-left: none;
  border-right: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
}
.myselfclass_top{
  margin-top: 40rpx;
  height: 350rpx;
  width: 100%;
}
.myselfclass{
  margin-top: 0rpx;
  height: 80rpx;
  width: 100%;
  background: #fff;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-left: none;
  border-right: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
 
}
.myselfleftclass{  
  color:#000000;
  text-indent:40rpx;
  text-align:justify;
}
.myselfrightclass{
  color: #000000;
  
}
    
.signUpButton{
  display: flex;
  align-items: center;
